<UserControl x:Class="HaenggiModel.Presentation.UserControls.PdfExporter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HaenggiModel.Presentation.UserControls"
             xmlns:Metrics="clr-namespace:HaenggiModel.Presentation.UsersControls.Metrics" mc:Ignorable="d" 
             xmlns:p="clr-namespace:HaenggiModel.Presentation.Properties"
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas HorizontalAlignment="Center" VerticalAlignment="Top" Background="Azure">
        <Image Source="..\Resources\Icons\Logo_Inverso_black.png" Width="50" Height="50" Canvas.Top="10" Canvas.Left="90"></Image>
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

        </Grid>
    </Canvas>
</UserControl>
